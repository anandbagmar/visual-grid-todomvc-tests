{
  "name": "visual-grid-todomvc-tests",
  "version": "1.0.0",
  "dependencies": {
    "@applitools/eyes-cypress": "^3.9.20",
    "@applitools/eyes-storybook": "^3.3.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "todomvc-app-css": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "storybook": "start-storybook -p 9009 -s public",
    "build": "react-scripts build && npm run build-storybook",
    "test": "npm-run-all -p 'test:*'",
    "test:cypress": "BROWSER=none npm-run-all -p -r start cypress-run",
    "test:eyes-storybook": "eyes-storybook",
    "test:eslint": "npm run eslint -- --fix",
    "cypress-run": "cypress run",
    "eslint": "eslint 'src/**/*.js' 'cypress/**/*.js' 'selenium/**/*.js'",
    "build-storybook": "build-storybook -s public",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-create-react-app": "^2.1.1",
    "@storybook/react": "^5.3.17",
    "chromedriver": "^80.0.1",
    "cypress": "^4.2.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-mocha-no-only": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "selenium-webdriver": "^4.0.0-alpha.7"
  }
}
